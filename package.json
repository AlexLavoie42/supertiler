{
    "name": "supertiler",
    "version": "0.0.4",
    "description": "GeoJSON -> MBTiles with Supercluster",
    "module": "main.js",
    "type": "module",
    "types": "supertiler.d.ts",
    "scripts": {
        "pretest": "eslint main.js supertiler.js test/test.js",
        "test": "tap test",
        "cov": "tap test --cov"
    },
    "bin": {
        "supertiler": "supertiler.js"
    },
    "repository": "https://github.com/ChrisLoer/supertiler",
    "author": "Chris Loer <chris.loer@gmail.com>",
    "license": "BSD",
    "keywords": [
        "clustering",
        "geospatial",
        "mbtiles"
    ],
    "dependencies": {
        "node-gzip": "^1.1.2",
        "sqlite": "^4.2.1",
        "sqlite3": "^5.1.6",
        "supercluster": "^8.0.1",
        "vt-pbf": "^3.1.3",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "eslint": "^8.44.0",
        "eslint-config-mourner": "^3.0.0",
        "tap": "^16.3.7"
    },
    "eslintConfig": {
        "extends": "mourner",
        "rules": {
            "camelcase": 0
        }
    },
    "exports": {
        ".": {
            "import": "./main.js",
            "require": "./main.cjs"
        }
    }
}
